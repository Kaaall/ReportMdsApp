# Laravel App Settings
APP_NAME=Laravel
APP_ENV=production
APP_KEY=base64:your-generated-app-key-here  # Ganti dengan hasil dari `php artisan key:generate`
APP_DEBUG=false
APP_TIMEZONE=Asia/Jakarta
APP_URL=https://your-app-name.up.railway.app  # Ganti dengan URL dari Railway

# Localization
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

# Logging
LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=info
LOG_SLACK_WEBHOOK_URL=
LOG_STDERR_FORMATTER=

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=your-database-host
DB_PORT=3306
DB_DATABASE=your-database-name
DB_USERNAME=your-database-username
DB_PASSWORD=your-database-password
DB_URL=

# Queue & Caching
QUEUE_CONNECTION=database
DB_QUEUE_CONNECTION=database
CACHE_STORE=database
DB_CACHE_CONNECTION=database
DB_CACHE_LOCK_CONNECTION=database
DB_CACHE_LOCK_TABLE=cache_locks

# Session Configuration
SESSION_DRIVER=database
SESSION_CONNECTION=database
SESSION_STORE=
SESSION_LIFETIME=120
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=false

# Memcached Configuration
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PERSISTENT_ID=
MEMCACHED_USERNAME=
MEMCACHED_PASSWORD=

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_URL=
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=null
REDIS_USERNAME=

# Mail Configuration
MAIL_MAILER=smtp
MAIL_SCHEME=null
MAIL_URL=
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"
MAIL_LOG_CHANNEL=mail-log-channel

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_URL=
AWS_ENDPOINT=

# Postmark Configuration
POSTMARK_TOKEN=
POSTMARK_MESSAGE_STREAM_ID=

# Resend Email API
RESEND_KEY=

# Slack Bot
SLACK_BOT_USER_OAUTH_TOKEN=
SLACK_BOT_USER_DEFAULT_CHANNEL=

# SQS Configuration
SQS_SUFFIX=

# Papertrail Logging
PAPERTRAIL_URL=
PAPERTRAIL_PORT=

# MYSQL SSL Configuration
MYSQL_ATTR_SSL_CA=

# Frontend Vite Configuration
VITE_APP_NAME="${APP_NAME}"
